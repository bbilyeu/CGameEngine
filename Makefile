clean_debug: 
	rm -f $(OBJ_DEBUG) $(OUT_DEBUG)
	rm -rf bin/Debug
	rm -rf $(OBJDIR_DEBUG)/libs/fmt
	rm -rf $(OBJDIR_DEBUG)/srv
	rm -rf $(OBJDIR_DEBUG)/phys
	rm -rf $(OBJDIR_DEBUG)/net
	rm -rf $(OBJDIR_DEBUG)/gui
	rm -rf $(OBJDIR_DEBUG)/draw
	rm -rf $(OBJDIR_DEBUG)/common
	rm -rf $(OBJDIR_DEBUG)
	rm -rf $(OBJDIR_DEBUG)/libs/Box2D/dynamics
	rm -rf $(OBJDIR_DEBUG)/libs/Box2D/common
	rm -rf $(OBJDIR_DEBUG)/libs/Box2D/rope
	rm -rf $(OBJDIR_DEBUG)/libs/Box2D/collision

before_release: 
	test -d bin/Release || mkdir -p bin/Release
	test -d $(OBJDIR_RELEASE)/libs/fmt || mkdir -p $(OBJDIR_RELEASE)/libs/fmt
	test -d $(OBJDIR_RELEASE)/srv || mkdir -p $(OBJDIR_RELEASE)/srv
	test -d $(OBJDIR_RELEASE)/phys || mkdir -p $(OBJDIR_RELEASE)/phys
	test -d $(OBJDIR_RELEASE)/net || mkdir -p $(OBJDIR_RELEASE)/net
	test -d $(OBJDIR_RELEASE)/gui || mkdir -p $(OBJDIR_RELEASE)/gui
	test -d $(OBJDIR_RELEASE)/draw || mkdir -p $(OBJDIR_RELEASE)/draw
	test -d $(OBJDIR_RELEASE)/common || mkdir -p $(OBJDIR_RELEASE)/common
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)
	test -d $(OBJDIR_RELEASE)/libs/Box2D/dynamics || mkdir -p $(OBJDIR_RELEASE)/libs/Box2D/dynamics
	test -d $(OBJDIR_RELEASE)/libs/Box2D/common || mkdir -p $(OBJDIR_RELEASE)/libs/Box2D/common
	test -d $(OBJDIR_RELEASE)/libs/Box2D/rope || mkdir -p $(OBJDIR_RELEASE)/libs/Box2D/rope
	test -d $(OBJDIR_RELEASE)/libs/Box2D/collision || mkdir -p $(OBJDIR_RELEASE)/libs/Box2D/collision

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) -shared $(LIBDIR_RELEASE) $(OBJ_RELEASE)  -o $(OUT_RELEASE) $(LDFLAGS_RELEASE) $(LIB_RELEASE)

